#include <iostream>


using namespace std;

int main()
{
    cout << "Hello World!" << endl;
    string str = "";
    string nom = "" ;
    string prenom = "";
    int compteur = 0 ;
    string humain = "";
    for (getline (cin, str); !cin.eof (); getline (cin, str))
    {
        if (compteur % 2 == 1)
        {
            prenom = prenom + " " + str ;
        }
        compteur += 1 ;
    }
    cout << prenom << endl;
}


